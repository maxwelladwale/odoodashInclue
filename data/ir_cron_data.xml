<odoo>
    <data noupdate="1">
        <record id="ir_cron_send_followup_reminders" model="ir.cron">
            <field name="name">Send Facilitation Follow-up Reminders</field>
            <field name="model_id" ref="model_facilitation_session"/>
            <field name="state">code</field>
            <field name="code">model._send_followup_reminders()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>